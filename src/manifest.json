{
  "manifest_version":3,
  "name":"XにXを爆誕させるやつ @ public version",
  "version":"1.0",
  "description":"ブラウザ版のX.comにXが爆誕します",
  "icons":{
    "32":"image/icon32.png",
    "48":"image/icon48.png",
    "128":"image/icon128.png"
  },
  "permissions":["activeTab","tabs"],
  "content_scripts":[
    {
      "matches":["https://x.com/*"],
      "js":["globals.js","hackXHR.js","loading.js"],
      "run_at":"document_start"
    },
    {
      "matches":["https://x.com/*"],
      "js":["addeffect.js","favicon.js", "hovorLogo.js","tweetButton.js","notificationBell.js","deleteGrokExplanation.js","replaceimage.js"]
    }
  ],
  "web_accessible_resources":[
    {
      "resources":["image/*", "anime/*", "sound/*", "scripttext/*", "file.json"],
      "matches":["https://x.com/*"]
    }
  ]
}